program 
  var N as int ;
  var M as bool ; 
  var SQRT as int ; 
begin 

  %SQRT := ( N + N )  ;
  %SQRT := N + ( 1 * 2 ) div 3 + 4  ;
  %SQRT := N + 10 * ( 9 * ( 12 + 13 ) ) ; 
  %SQRT := N * 10 div 11 * ( 12 + 13 ) - 14 ;
  %SQRT := 1 + 2 ;
  SQRT := 1 + 2 - 3 ;
  %N := 1 + 2 - 3 + 4 - 5 + 6 ;
  while SQRT + SQRT <= N + N - N do 
    SQRT := SQRT + 1 ; 
  end ;
  SQRT := 1 + 2 * 3 ;
end 
